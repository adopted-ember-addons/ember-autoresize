    "ember-cli-version-checker": "^2.1.0",
    "ember-get-config": "^0.2.4"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "~3.0.0",
    "ember-cli-addon-docs": "^0.3.0",
    "ember-cli-addon-docs-yuidoc": "^0.1.1",
    "ember-cli-dependency-checker": "^2.0.0",
